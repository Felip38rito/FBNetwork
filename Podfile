target 'FBNetwork' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FBNetwork
  pod 'FBModel', :git => 'https://github.com/Felip38rito/FBModel.git', :tag => '0.0.10'

  target 'FBNetworkTests' do
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
  end

end
